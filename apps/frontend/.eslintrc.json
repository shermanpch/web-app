{
  "extends": ["next/core-web-vitals", "eslint:recommended"],
  "root": true,
  "globals": {
    "React": "readonly"
  },
  "rules": {
    "react/react-in-jsx-scope": "off",
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_",
        "ignoreRestSiblings": true
      }
    ]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
